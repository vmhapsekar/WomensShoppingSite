    "url-loader": "^0.5.9",
    "uuid": "^3.1.0",
    "wdio-allure-reporter": "^0.1.2",
    "wdio-cucumber-framework": "1.1.0",
    "wdio-cucumber-reporter": "^0.0.2",
    "wdio-dot-reporter": "^0.0.9",
    "wdio-json-reporter": "^0.2.1",
    "wdio-junit-reporter": "^0.3.1",
    "wdio-phantomjs-service": "~0.2.2",
    "wdio-selenium-standalone-service": "^0.0.9",
    "wdio-simple-reporter": "^0.1.2",
    "wdio-spec-reporter": "^0.1.2",
    "wdio-sync": "^0.7.0",
    "wdio-teamcity-reporter": "^1.1.1",
    "webdriverajax": "^2.0.0",
    "webdriverio": "~4.8.0",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.7.1"
  },
  "git": {
    "scripts": {
      "pre-commit": "yarn standard"
    }
  },
  "standard": {
    "ignore": [
      "/node_modules/",
      "app/src/assets",
      "yarn-local.js",
      "retry.js",
      "/app/e2e/publish/",
      "/app/vendor_libs/",
      "sus6pzy.min.js",
      "**/*.spec.js"
    ],
    "globals": [
      "it",
      "fail",
      "console",
      "protractor",
      "browser",
      "by",
      "element",
      "expect",
      "jest",
      "describe",
      "xdescribe",
      "assert",
      "test",
      "beforeEach",
      "afterEach",
      "beforeAll",
      "afterAll"
    ]
  },
  "jest-html-reporter": {
    "pageTitle": "BDS Unit Test Suite",
    "outputPath": "./reports/unit/BDS-unit-test-report.html",
    "includeFailureMsg": true,
    "theme": "lightTheme"
  },
  "jest": {
    "collectCoverage": true,
    "testRegex": "\\.(spec)\\.js?$",
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/app/__mocks__/sessionStorage.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/app/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/app/__mocks__/cssMock.js"
    },
    "globals": {
      "ENV": "DEV",
      "NOWWHERE_SERVICE_API_KEY": "TEST_KEY"
    },
    "cacheDirectory": "tmp/",
    "testPathIgnorePatterns": [],
    "coverageDirectory": "./reports/coverage/jest/",
    "collectCoverageFrom": [
      "app/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "app/src/index.js",
      "app/storybook.js",
      "app/config",
      "app/e2e",
      "spec.js",
      ".stories.js",
      "/assets/",
      "app/src/styles"
    ],
    "coverageReporters": [
      "json",
      "html",
      "lcov",
      "text"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 85,
        "branches": 85,
        "functions": 85,
        "lines": 85
      }
    },
    "verbose": false,
    "transform": {
      "^.+\\.js?$": "babel-jest"
    },
    "setupTestFrameworkScriptFile": "./app/config/jest/jest.hooks.js",
    "testResultsProcessor": "./app/config/jest/resultsProcessor"
  },
  "devDependencies": {
    "jest-styled-components": "^4.4.1",
    "raf": "^3.4.0"
  }
}
