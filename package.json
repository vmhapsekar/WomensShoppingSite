{
  "name": "NPP-BDS-UI",
  "version": "1.0.0",
  "description": "NPP Bankers Desktop - Service",
  "main": "app.js",
  "author": "Pokemon squad",
  "license": "MIT",
  "scripts": {
    "link-modules": "yarn --pure-lockfile --offline",
    "start": "NODE_ENV=development webpack-dev-server --inline --hot",
    "dev": "NODE_ENV=dev_cache webpack-dev-server --inline --hot",
    "build": "webpack --config ./webpack.config.prod.js",
    "a11y": "eslint  app/**/*",
    "a11y:watch": "esw   app/**/*",
    "lint": "standard --fix | snazzy",
    "lintWithoutFix": "standard | snazzy",
    "fix": "standard --fix",
    "test": "standard && jest",
    "test-ci": "NODE_ENV=test mkdir -p reports/unit && jest && node ./yarn-local.js coverage",
    "justtest": "NODE_ENV=test mkdir -p reports/unit && jest",
    "watch": "jest --watch --collectCoverage=false",
    "e2etest": "wdio ./e2e/config/wdio.conf.js",
    "update-snapshot": "NODE_ENV=test jest --updateSnapshot",
    "coverage": "istanbul check-coverage"
  },
  "dependencies": {
    "axios": "0.16.2",
    "axios-mock-adapter": "^1.8.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^6.4.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "~6.24.0",
    "bluebird": "^3.5.0",
    "chai": "~3.5.0",
    "chai-shallow-deep-equal": "^1.4.6",
    "chai-subset": "^1.5.0",
    "classnames": "^2.2.5",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.0",
    "cucumber": "~2.3.1",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "enzyme-to-json": "^3.0.1",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-jsx": "^4.0.1",
    "eslint-config-standard-react": "5.0.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-jsx-a11y": "~5.0.3",
    "eslint-watch": "^3.1.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.1",
    "git-scripts": "^0.2.1",
    "history": "^4.7.2",
    "html-react-parser": "^0.4.1",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy-middleware": "^0.17.4",
    "immutability-helper": "^2.2.3",
    "istanbul": "^0.4.5",
    "jest": "^21.2.1",
    "jest-css-modules": "^1.1.0",
    "jest-html-reporter": "^1.2.0",
    "jest-junit": "^1.5.1",
    "jest-sonar-reporter": "^1.1.1",
    "json-loader": "^0.5.4",
    "jsonp": "^0.2.1",
    "konfig": "^0.2.1",
    "latest": "^0.2.0",
    "lodash": "^4.17.4",
    "lodash.assign": "^4.2.0",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.13",
    "mountebank": "^1.11.0",
    "node-rest-client": "^3.1.0",
