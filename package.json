    "postcss": "^5.2.17",
    "postcss-cssnext": "3.0.2",
    "postcss-extend": "^1.0.5",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.3",
    "postcss-nested": "^1.0.1",
    "prop-types": "^15.5.8",
    "radium": "^0.19.5",
    "react": "16",
    "react-dom": "^16.0.0",
    "react-hot-loader": "next",
    "react-inspector": "2.2.0",
    "react-modal": "2.3.2",
    "react-redux": "^5.0.6",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-styled-flexboxgrid": "^2.0.3",
